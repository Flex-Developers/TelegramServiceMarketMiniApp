{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=telegram_marketplace;Username=postgres;Password=your_password",
    "Redis": "",
    "Sqlite": "Data Source=mydatabase.db"
  },
  "Telegram": {
    "BotToken": "",
    "BotUsername": "@flexdevelopersBot",
    "WebAppUrl": "https://market.flex-developers.com/"
  },
  "JWT": {
    "Secret": "your-super-secret-key-at-least-32-characters-long",
    "Issuer": "TelegramMarketplace",
    "Audience": "TelegramMarketplaceUsers",
    "ExpiryMinutes": 60
  },
  "PaymentProviders": {
    "YooKassa": {
      "ShopId": "YOUR_SHOP_ID",
      "SecretKey": "YOUR_SECRET_KEY",
      "WebhookSecret": "YOUR_WEBHOOK_SECRET",
      "ReturnUrl": "https://your-webapp-url.com/payment/success",
      "IsTestMode": true
    },
    "Robokassa": {
      "MerchantLogin": "YOUR_MERCHANT_LOGIN",
      "Password1": "YOUR_PASSWORD_1",
      "Password2": "YOUR_PASSWORD_2",
      "IsTestMode": true
    }
  },
  "Commission": {
    "Percentage": 10
  },
  "Redis": {
    "InstanceName": "TelegramMarketplace:"
  },
  "FileStorage": {
    "BasePath": "uploads",
    "BaseUrl": "/uploads",
    "MaxFileSizeBytes": 10485760,
    "AllowedExtensions": [".jpg", ".jpeg", ".png", ".gif", ".webp"]
  }
}
